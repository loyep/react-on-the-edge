{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "nestjs-app",
  "buildCommand": "pnpm build",
  "functions": {
    "api/vercel.ts": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/vercel.ts"
    }
  ],
  "outputDirectory": "dist"
}
